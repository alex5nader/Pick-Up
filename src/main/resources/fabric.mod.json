{
  "schemaVersion": 1,

  "id": "${modId}",
  "version": "${modVersion}",
  "environment": "*",

  "name": "${modPrettyName}",
  "description": "",
  "icon": "assets/${modId}/icon.png",

  "authors": [ "noobstar" ],
  "contributors": [],
  "contact": {
    "sources": "https://github.com/alex5nader/${modName}",
    "issues": "https://github.com/alex5nader/${modName}/issues",
    "email": "${modId}@alexnader.dev",
    "discord": "noob2star#7501"
  },

  "license": "LGPL-3.0-or-later",

  "entrypoints": {
    "main": [
      "dev.alexnader.pick_up.common.PickUp"
    ],
    "client": [
      "dev.alexnader.pick_up.client.PickUpClient"
    ]
  },
  "mixins": [
    "mixins.${modId}.json"
  ],

  "custom": {
    "pick_up": {
      "denied_blocks": [
        "minecraft:white_bed",
        "minecraft:orange_bed",
        "minecraft:magenta_bed",
        "minecraft:light_blue_bed",
        "minecraft:yellow_bed",
        "minecraft:lime_bed",
        "minecraft:pink_bed",
        "minecraft:gray_bed",
        "minecraft:light_gray_bed",
        "minecraft:cyan_bed",
        "minecraft:purple_bed",
        "minecraft:blue_bed",
        "minecraft:brown_bed",
        "minecraft:green_bed",
        "minecraft:red_bed",
        "minecraft:black_bed"
      ],
      "denied_entities": [
        "minecraft:player"
      ]
    }
  },

  "depends": {
    "fabricloader": "*",
    "fabric": "${fapiVersion}",
    "minecraft": ["${mcVersions}"]
  }
}